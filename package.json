{
  "name": "my-react-profile",
  "author": "Pramod",
  "description": "React powered Job Profile",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.7.0",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.6",
    "firebase": "^3.6.1",
    "lodash": "^4.17.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-lazy-load": "^3.0.13",
    "react-pose": "^4.0.4",
    "react-scrollreveal": "^1.2.0",
    "scrollreveal": "^4.0.5"
  },
  "devDependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
